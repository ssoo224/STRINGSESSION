{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python main.py"
  },
  "env": {
    "BOT_TOKEN": {
      "description": "Get this value from @BotFather",
      "required": true
    },
    "MUST_JOIN": {
      "description": "Username of a group or channel for which you want to set force sub. (without @)",
      "required": true
    },
    "MONGO_URL": {
      "description": "MongoDB URL here",
      "required": true
    },
    "API_ID": {
      "description": "Telegram API ID here",
      "required": true
    },
    "OWNER_ID": {
      "description": "Telegram ID of the person who will be the bot owner",
      "required": true
    },
    "API_HASH": {
      "description": "Telegram API hash here",
      "required": true
    }
  }
}
